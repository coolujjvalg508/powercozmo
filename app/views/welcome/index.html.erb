<!-- Banner Start -->
<section class="banner clearfix">
	<div class="container">
		<div class="row">
			<h2 class="text-center text-uppercase"><%= SiteSetting.last.try(:homepage_heading) || 'Trading platform' %></h2>
			<div class="text-center banner-txt text-uppercase">
				<%= SiteSetting.last.try(:homepage_text) || 'for used and surplus of capital power equipment and power generation plants, providing effective sources and logistics solutions to buyers and sellers' %>
			</div>
			
<%= form_tag listing_filter_path, method: :get do %>
			
			<div class="clearfix">
				<div class="col-md-10 col-sm-12 col-xs-12 col-md-offset-1">
					<div class="col-md-3 col-sm-4 col-xs-12 padding">
						<div class="cd-dropdown-wrapper"> 
							
							<div class="cd-dropdown-trigger clearfix">
							  
							  <%= select_tag "filter[category_id]", options_from_collection_for_select(@categories, "id", "name"), include_blank: 'Select Category', class: 'custom-select' %>
							  
							</div>
          
						</div>
					</div>
					
					<div class="col-md-7 col-sm-5 col-xs-12 padding">
						
						<%= search_field_tag "q[name_or_identifier_or_country_name_or_description_or_manufacturer_name_or_sub_category_name_or_sub_sub_category_name_cont]", "", placeholder: "Search Name, Identifier, Manufacturer, Country", title: "Search Name, Identifier, Manufacturer, Country", class: "form-control" %>
        						
					</div>
					<div class="col-md-2 col-sm-3 col-xs-12 padding">
						<button class="btn-orange text-uppercase"><i class="fa fa-search" aria-hidden="true"></i> search </button>
					</div>
				</div>
			</div>
			
<% end %>			
				
		</div>
	</div>
</section>

<div class="clearfix postswrap">
	
	<%= link_to '<i class="fa fa-pencil" aria-hidden="true"></i> post an ad'.html_safe, new_seller_equipment_path, class: 'post-btn text-uppercase text-center' %>
	
</div>


